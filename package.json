{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:flutter": "cd ../flutter && flutter build web --release --base-href /app/ && rm -rf ../next/public/app && mkdir -p ../next/public/app && cp -r build/web/* ../next/public/app/",
    "build:all": "npm run build:flutter && npm run build",
    "clean": "rm -rf .next dist public/app node_modules",
    "serve": "npx serve dist"
  },
  "dependencies": {
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "serve": "^14.2.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
